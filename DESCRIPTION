Package: epocakir
Title: Clinical Coding of Patients with Kidney Disease using Clinical Practice
    Guidelines
Version: 0.4.0
Authors@R:
    c(person(given = "Alwin",
             family = "Wang",
             role = c("aut", "cre"),
